{
  "name": "svelte-spinner",
  "version": "2.0.1",
  "description": "A Svelte SVG loading spinner",
  "svelte": "src/index.svelte",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "author": "Emil Tholin @tholle1234, Asko Kauppi",
  "license": "MIT",
  "repository": "akauppi/svelte-spinner",
  "scripts": {
    "build": "rollup -c",
    "dev": "run-p dev:start dev:watch",
    "dev:watch": "rollup -c -w",
    "dev:start": "sirv app/public --dev --single",
    "clean": "rm app/public/*.js app/public/*.css app/public/*.map"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rollup": "^1.27.4",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "sirv-cli": "^0.4.5",
    "svelte": "^3.15.0"
  }
}
